{
  "plugins": ["prettier"],
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "env": {
    "jest": true
  },
  "rules": {
    "import/prefer-default-export": "off",
    "import/extensions": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "import/no-extraneous-dependencies": "off"
  }
}
