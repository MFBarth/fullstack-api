{
    "openapi": "3.0.0",
    "info": {
        "title": "Fullstack API",
        "description": " Manipulação de Usuários",
        "termsOfService": "http://localhost:3333/",
        "contact": {
            "email": "matheusbarthf@gmail.com"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:3333/",
            "description": "FullStack Api"
        }
    ] ,
    "paths": {
        "/api/users/create": {
            "post": {
                "summary": "Create user",
                "description": "This can only be done by the logged in user.",
                "tags": ["User"],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/CreateUser"
                            }
                        }
                    }
                },
                "responses": {
                    "400": {
                        "description": "Não foi possível criar o usuário"
                    },
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/User"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/users/update": {
            "put": {
                "summary": "Update user",
                "description": "This can only be done by the logged in user.",
                "tags": ["User"],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/User"
                            }
                        }
                    }
                },
                "responses": {
                    "400": {
                        "description": "Não foi possível atualizar o usuário"
                    },
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/User"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/users/find": {
            "get": {
                "summary": "Get user",
                "description": "This can only be done by the logged in user.",
                "tags": ["User"],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/FindUser"
                            }
                        }
                    }
                },
                "responses": {
                    "400": {
                        "description": "Não foi possível buscar o usuário"
                    },
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/User"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/users/delete": {
            "delete": {
                "summary": "Delete user",
                "description": "This can only be done by the logged in user.",
                "tags": ["User"],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/FindUser"
                            }
                        }
                    }
                },
                "responses": {
                    "400": {
                        "description": "Não foi possível deletar o usuário"
                    },
                    "200": {
                        "description": "Usuário foi deletado com sucesso."
                    }
                }
            }
        },
        "/api/users/findAll": {
            "get": {
                "summary": "Find All users",
                "description": "This can only be done by the logged in user.",
                "tags": ["User"],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/User"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/session/": {
            "post": {
                "summary": "Create Session",
                "description": "This can only be done by the logged in user.",
                "tags": ["Session"],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Session"
                            }
                        }
                    }
                },
                "responses": {
                    "400": {
                        "description": "Não foi possível criar a seção"
                    },
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "accessToken": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "CreateUser": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "lastName": {
                        "type": "string"
                    },
                    "cpf": {
                        "type": "string"
                    },
                    "phone": {
                        "type": "string"
                    },
                    "birthDay": {
                        "type": "string"
                    }
                }
            },
            "User": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "lastName": {
                        "type": "string"
                    },
                    "cpf": {
                        "type": "string"
                    },
                    "phone": {
                        "type": "string"
                    },
                    "birthDay": {
                        "type": "string"
                    }
                }
            },
            "FindUser": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    }
                }
            },
            "Session": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    }
                }
            }
        }
    }
}